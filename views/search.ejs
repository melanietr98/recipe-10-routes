<!DOCTYPE html>
<html>
<head>
  <title>Search Results</title>
</head>
<body>
  <h1>Search Results for "<%= searchTerm %>"</h1>
  <% if (searchResults.length > 0) { %>
    <ul>
      <% searchResults.forEach(function(result) { %>
        <li><a href="/recipe/<%= result.id %>"><%= result.title %></a></li>
      <% }); %>
    </ul>
  <% } else { %>
    <p>No results found.</p>
  <% } %>
</body>
</html>
